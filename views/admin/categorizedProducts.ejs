<%- include('../partials/admin-header'); %>

    <div class="col-lg-10 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Categorized Products</h4>
                <div class="table-responsive">
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th>
                                    Image
                                </th>
                                <th>
                                    Product name
                                </th>
                                <th>
                                    Brand
                                </th>
                                <th>
                                    Quantity
                                </th>
                                <th>
                                    Category
                                </th>
                                <th>
                                    Prize
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            <% result.forEach(function(result){ %>
                                <tr>
                                    <td class="py-1">
                                        <img src="/productImages/<%= result.image[0] %>" alt="image" />
                                    </td>
                                    <td>
                                        <%= result.name %>
                                    </td>
                                    <td>
                                        <%= result.brand %>
                                    </td>
                                    <td>
                                        <%= result.quantity %>
                                    </td>
                                    <td>
                                        <%= result.category %>
                                    </td>
                                    <td>
                                        <%= result.price %>
                                    </td>
                                </tr>
                                <% }); %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

    <%- include('../partials/admin-footer'); %>