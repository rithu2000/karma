							<p>These are our Latest and newest products</p>
						</div>
					</div>
				</div>
				<div class="row">
					<!-- single product -->
					<% result.forEach(function(result){ %>
						<div class="col-lg-3 col-md-6">
							<div class="single-product">
								<a href="/productDetails/<%= result._id %>">
									<img src="productImages/<%= result.image[0] %>" width="200px" height="280px"
										alt=""></a>
								<div class="product-details">
									<h6>
										<%= result.name %>
									</h6>
									<div class="price">
										<h6>Rs. <%= result.price %>
										</h6>
									</div>
									<div class="prd-bottom">
										<a href="/cart/<%= result._id %>" class="social-info">
