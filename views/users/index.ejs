	<%- include('../partials/footer'); %>

		<script>
			function addToWishlist(proId) {
				$.ajax({
					url: '/wishlist/' + proId,
					method: 'post',
					success: (response) => {
						if (response.status) {

							location.reload()
							
						} else {
							Swal.fire({
								icon: 'info',
								title: 'Please Login',
								showConfirmButton: false,
								timer: 1000
							})
						}
					}
				})
			}
		</script>